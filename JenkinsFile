pipeline {
	
	agent any
	
    stages {
        stage('Build') {
            steps {
				echo 'Installing dependencies'
                npm install
				echo 'Starting build'
				npm run build
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
				npm run test
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
