pipeline {
	
	agent any
	
    stages {
		stage('Prepare') {
			steps {
				echo 'Starting build'
				sh 'npm run build'
			}
		}
        stage('Build') {
            steps {
				echo 'Installing dependencies'
                sh 'npm install'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
				sh 'npm run test'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
