pipeline {
	
	agent any
	
    stages {
		stage('Prepare') {
			steps {
				echo 'Starting build'
				npm run build
			}
		}
        stage('Build') {
            steps {
				echo 'Installing dependencies'
                npm install
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
				npm run test
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
